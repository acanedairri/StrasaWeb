<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.strasa.middleware.mapper.other.StudyRawDataBatch">
<insert id="insertBatchRaw" parameterType="java.util.List">

   INSERT INTO studyrawdata (id,studyid,dataset,datarow,datacolumn,datavalue)
        VALUES
        <foreach collection="list" item="item" open="(" separator="),(" close=")">
           #{item.id},#{item.studyid},#{item.dataset},#{item.datarow},#{item.datacolumn},#{item.datavalue}
        </foreach>
</insert>
<insert id="insertBatchDerived" parameterType="java.util.List">

   INSERT INTO studyderiveddata (id,studyid,dataset,datarow,datacolumn,datavalue)
        VALUES
        <foreach collection="list" item="item" open="(" separator="),(" close=")">
           #{item.id},#{item.studyid},#{item.dataset},#{item.datarow},#{item.datacolumn},#{item.datavalue}
        </foreach>
</insert>
</mapper>
